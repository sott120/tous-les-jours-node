                    <label for="userNm" class="hid">이름</label>
                    <input type="text" class="user_nm" name="user_nm" id="userNm" placeholder="이름을 입력하세요." maxlength="10" />
                    <p class="input_warn_txt"></p>
                </div>
                <div class="left">
                    <label for="userGender" class="hid">성별</label>
                    <div class="radio_wrap">
                        <input class="radio radio_left" type="radio" name="user_gender" value="남" />
                    </div>
                    <div class="radio_wrap">
                        <input class="radio radio_right" type="radio" name="user_gender" value="여" />
                    </div>
                </div>
            </div>
